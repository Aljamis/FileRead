<FileLayout>
	<record classname="org.avr.Member" >
		<field name="firstName" trim="true" occurs="1">
			<type>String</type>
			<start>1</start>
			<end>30</end>
		</field>
		<field name="lastName" trim="true">
			<type>String</type>
			<start>31</start>
			<end>60</end>
		</field>
		
		<megaField classname="org.avr.Address" name="homeAddr">
			<field name="addr1" trim="false">
				<type>String</type>
				<start>61</start>
				<end>90</end>
			</field>
			<field name="addr2" trim="false">
				<type>String</type>
				<start>91</start>
				<end>130</end>
			</field>
			<field name="city" trim="false">
				<type>String</type>
				<start>131</start>
				<end>160</end>
			</field>
			<field name="state" trim="false">
				<type>String</type>
				<start>161</start>
				<end>162</end>
			</field>
			<!--  field name="zip" trim="false">
				<type>String</type>
				<start>163</start>
				<end>172</end>
			</field  -->
			<megaField classname="org.avr.ZipCode" name="zipCode">
				<field name="zip" trim="false">
					<type>String</type>
					<start>163</start>
					<end>167</end>
				</field>
				<field name="zip4" trim="false">
					<type>String</type>
					<start>168</start>
					<end>171</end>
				</field>
			</megaField>
		</megaField>
		
		<field name="email" trim="false">
			<type>String</type>
			<start>172</start>
			<end>224</end>
		</field>
		
		<field name="dob" trim="false">
			<type format="yyyyMMdd">date</type>
			<start>225</start>
			<end>232</end>
		</field>
		
	</record>
</FileLayout>