<FileLayout>
	<header classname="org.avr.MyHeader" uid="H" uidStart="0" uidEnd="1">
		<field name="fileType" trim="true">
			<type>String</type>
			<start>2</start>
			<end>11</end>
		</field>
		<field name="runDate" >
			<type format="yyyyMMdd" >date</type>
			<start>12</start>
			<end>19</end>
		</field>
		<field name="recCount" trim="true" >
			<type>int</type>
			<start>20</start>
			<end>30</end>
		</field>
	</header>


	<record classname="org.avr.Member" uid="M" uidStart="0" uidEnd="1">
		<field name="firstName" trim="true" occurs="1">
			<type>String</type>
			<start>2</start>
			<end>30</end>
		</field>
		<field name="lastName" trim="true">
			<type>String</type>
			<start>31</start>
			<end>60</end>
		</field>
		
		<megaField classname="org.avr.Address" name="homeAddr">
			<field name="addr1" trim="false">
				<type>String</type>
				<start>61</start>
				<end>90</end>
			</field>
			<field name="addr2" trim="false">
				<type>String</type>
				<start>91</start>
				<end>130</end>
			</field>
			<field name="city" trim="false">
				<type>String</type>
				<start>131</start>
				<end>160</end>
			</field>
			<field name="state" trim="false">
				<type>String</type>
				<start>161</start>
				<end>162</end>
			</field>
			<!--  field name="zip" trim="false">
				<type>String</type>
				<start>163</start>
				<end>172</end>
			</field  -->
			<megaField classname="org.avr.ZipCode" name="zipCode">
				<field name="zip" trim="false">
					<type>String</type>
					<start>163</start>
					<end>167</end>
				</field>
				<field name="zip4" trim="false">
					<type>String</type>
					<start>168</start>
					<end>171</end>
				</field>
			</megaField>
		</megaField>
		
		<field name="email" trim="false">
			<type>String</type>
			<start>172</start>
			<end>224</end>
		</field>
		
		<field name="dob" trim="false">
			<type format="yyyyMMdd">date</type>
			<start>225</start>
			<end>232</end>
		</field>
		
	</record>
	
	
	<!--
		Similar to member but the first name and last name are flipped.
		this is to test multiple record types. 
	 -->
	<record classname="org.avr.Spouse" uid="S" uidStart="0" uidEnd="1">
		<field name="lastName" trim="true" occurs="1">
			<type>String</type>
			<start>2</start>
			<end>30</end>
		</field>
		<field name="firstName" trim="true">
			<type>String</type>
			<start>31</start>
			<end>60</end>
		</field>
		
		<megaField classname="org.avr.Address" name="homeAddr">
			<field name="addr1" trim="false">
				<type>String</type>
				<start>61</start>
				<end>90</end>
			</field>
			<field name="addr2" trim="false">
				<type>String</type>
				<start>91</start>
				<end>130</end>
			</field>
			<field name="city" trim="false">
				<type>String</type>
				<start>131</start>
				<end>160</end>
			</field>
			<field name="state" trim="false">
				<type>String</type>
				<start>161</start>
				<end>162</end>
			</field>
			<!--  field name="zip" trim="false">
				<type>String</type>
				<start>163</start>
				<end>172</end>
			</field  -->
			<megaField classname="org.avr.ZipCode" name="zipCode">
				<field name="zip" trim="false">
					<type>String</type>
					<start>163</start>
					<end>167</end>
				</field>
				<field name="zip4" trim="false">
					<type>String</type>
					<start>168</start>
					<end>171</end>
				</field>
			</megaField>
		</megaField>
		
		<field name="email" trim="false">
			<type>String</type>
			<start>172</start>
			<end>224</end>
		</field>
		
		<field name="dob" trim="false">
			<type format="yyyyMMdd">date</type>
			<start>225</start>
			<end>232</end>
		</field>
		
	</record>

	<trailer classname="org.avr.MyTrailer" uid="TRLR" uidStart="0" uidEnd="4">
		<field name="fileType" trim="true">
			<type>String</type>
			<start>5</start>
			<end>14</end>
		</field>
		<field name="runDate" >
			<type format="yyyyMMdd" >date</type>
			<start>15</start>
			<end>22</end>
		</field>
		<field name="recCount" trim="true" >
			<type>int</type>
			<start>23</start>
			<end>33</end>
		</field>
	</trailer>


</FileLayout>